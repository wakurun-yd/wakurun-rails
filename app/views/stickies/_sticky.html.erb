<div class="col-md-2">
  <div class="thumbnail" style="background: #9F9">
    <div class="caption">
      <h3><%= sticky.comment %></h3>
      <span>@<%= sticky.author %></span>
    </div>
    <%= bootstrap_form_for([@board, sticky]) do |fl| %>
      <%= sticky.like %> Liked
      <%= fl.submit "+1", name: "like_btn", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
